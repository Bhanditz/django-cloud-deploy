{% raw %}{% extends "admin/base_site.html" %}
{% block branding %}
<h1 id="site-name"><a href="{% url 'admin:index' %}">Cloud Admin</a></h1>
{% endblock %}
{% block content %}
<h1>Logs</h1>
  {% if logs %}
  <ul>
    {% for log in logs %}
    <li><{{ log }}</li>
    {% endfor %}
  </ul>
  {% else %}
    <p>No logs are available.</p>
  {% endif %}
{% endblock %}
{% endraw %}